{
  "name": "surreal-notebook",
  "displayName": "surreal-notebook",
  "description": "Notebooks for SurrealQL",
  "version": "1.0.1",
  "icon": "surrealdb-icon.png",
  "publisher": "mathe42",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [
    "onNotebook:surreal.nb"
  ],
  "author": {
    "name": "mathe42",
    "email": "2pi_r2@gmx.de",
    "url": "https://github.com/mathe42"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathe42/suurreal-notebook"
  },
  "homepage": "https://github.com/mathe42/suurreal-notebook#readme",
  "bugs": {
    "url": "https://github.com/mathe42/suurreal-notebook/issues"
  },
  "main": "./out/extension.js",
  "contributes": {
    "notebooks": [
      {
        "id": "surreal.nb",
        "type": "surreal.nb",
        "displayName": "SurrealQL Notebook",
        "selector": [
          {
            "filenamePattern": "*.srqlnb"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/mocha": "9.1.1",
    "@types/node": "14.18.31",
    "@types/vscode": "1.71.0",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vscode/test-electron": "2.1.5",
    "eslint": "8.24.0",
    "glob": "8.0.3",
    "mocha": "9.2.2",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "mathe42.surrealdb.wasm": "^0.0.1",
    "surrealdb.wasm": "^0.1.0"
  }
}
